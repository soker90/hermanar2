{
  "build": {
    "beforeDevCommand": "pnpm dev-web",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "Hermanar",
  "version": "0.1.0",
  "identifier": "com.hermanar.app",
  "plugins": {
    "process": {
      "active": true
    }
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "Hermanar - Sistema de Gesti√≥n de Hermandades",
        "width": 1280,
        "height": 800,
        "dragDropEnabled": false,
        "maximized": true
      }
    ]
  }
}